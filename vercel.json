{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.go",
      "use": "@vercel/go",
      "config": {
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.go",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/webhook.go",
      "status": 404
    }
  ]
}